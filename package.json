{
  "name": "boilerplate-react-node",
  "version": "1.0.0",
  "description": "Boilerplate React Node",
  "main": "./app/src/index.js",
  "repository": "https://github.com/vitorblsantos/boilerplate-react-node",
  "author": "Vitor Santos",
  "license": "MIT",
  "private": false,
  "keywords": [
    "Boilerplate",
    "Boilerplate React",
    "Boilerplate Node",
    "Boilerplate React Node"
  ],
  "scripts": {
    "build": "concurrently --kill-others-on-fail \"webpack -p --progress\" \"sucrase ./server -d ./dist --transforms imports\"",
    "start": "concurrently --kill-others-on-fail \"webpack-dev-server --open --mode development -d\" \"sucrase-node ./server/\"",
    "server": "sucrase-node ./server/",
    "server:debug": "nodemon --inspect sucrase-node ./server/",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "verbose": true
  },
  "dependencies": {
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "concurrently": "^5.3.0",
    "core-js": "^3.6.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "fs": "^0.0.1-security",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-is": "^16.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@types/axios": "^0.14.0",
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.7",
    "@types/dotenv": "^8.2.0",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/express": "^4.17.7",
    "@types/jest": "^26.0.10",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@types/react-test-renderer": "^16.9.3",
    "@types/webpack": "^4.41.21",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.1.0",
    "chokidar": "^3.4.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.3",
    "eslint": "^7.6.0",
    "eslint-plugin-react": "^7.20.5",
    "html-webpack-plugin": "^4.3.0",
    "jest": "^26.3.0",
    "nodemon": "^2.0.4",
    "path": "^0.12.7",
    "react-test-renderer": "^16.13.1",
    "sucrase": "^3.15.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "resolutions": {
    "chokidar": "^3.4.0"
  }
}
